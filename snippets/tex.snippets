snippet paperd "Paper Layout for German text"
	\\documentclass[a4paper,11pt]{article}
	\\usepackage[utf8]{inputenc}
	\\usepackage[ngerman]{babel}
	\\usepackage[T1]{fontenc}
	\\usepackage{amsmath}
	\\usepackage{amsfonts}
	\\usepackage{graphicx}
	\\usepackage{fancyhdr}
	\\usepackage{paralist}
	\\usepackage{geometry}
		\\geometry{left=2.5cm, right=2.5cm, top=2.5cm, bottom=2cm}
	\\usepackage[onehalfspacing]{setspace}

	\\pagestyle{fancy}

	\\lhead{${1:Title}}
	\\rhead{Jirayu Ruh, 3cNP, KSBG St. Gallen}

	\\lfoot{\copyright Jirayu Ruh, 3cNP, KSBG St. Gallen}
	\\cfoot{}
	\\rfoot{\thepage}

	\\setcounter{secnumdepth}{5}
	\\setcounter{tocdepth}{5}

	\\setlength{\parindent}{0pt}

	\\begin{document}

	\\begin{titlepage}
		\\centering
		{\\LARGE \\textbf{$1} \\par}
		\\vspace{2cm}
		{\\Large \\textbf{These:} \\par}
		{\\Large \\textbf{${2:Thesis} \\par}
		\\vspace{2cm}
		{\\Large Jirayu Ruh\\par}
		\\vspace{0.5cm}
		{\\Large Kantonsschule am Burggraben St. Gallen\\par}
		\\vspace{0.5cm}
		{\\Large ${3:Date}\\par}
		\\vspace{5cm}
		{\\Large Word count: XXX}
	\\end{titlepage} 

	\\tableofcontents
	
	\\newpage

	\\section{${4:Title}}
	
	Insert your Text here

	\\end{document}

