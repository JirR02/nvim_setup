snippet pppd "PowerPoint Layout for German text"
	\\documentclass{beamer}

	%\\mode<presentation>{\\usetheme{}}

	\\usepackage[utf8]{inputenc}
	\\usepackage[ngerman]{babel}
	\\usepackage{graphicx}
	\\usepackage{booktabs}

	\\title{${1:Title}}
	\\author{${2:Groupmembers}}
	\\institute{KSBG, St. Gallen}
	\\date{\today}

	\\begin{document}

	\\begin{frame}
	\\titlepage
	\\end{frame}

	\\begin{frame}
	\\frametitle{Inhalt}
	\\tableofcontents
	\\end{frame}

	\\section{Section 1}

	\\end{document}

